{
  "name": "territory-api",
  "version": "1.0.0",
  "description": "API Rest to assigning of territories registry",
  "main": "src/app.ts",
  "scripts": {
    "start": "node app.js",
    "start:dev": "ts-node-dev src/app.ts",
    "build": "npm run compile && npm run copy-assets",
    "compile": "rimraf ./dist && tsc",
    "copy-assets": "npx copyfiles package.json ./dist && npx copyfiles *.lock ./dist && npx copyfiles README.md ./dist && npx copyfiles ./prisma/* ./dist && npx copyfiles ./prisma/**/* ./dist && npx copyfiles ./prisma/**/**/* ./dist",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "generate-db": "npx prisma migrate deploy"
  },
  "keywords": [
    "assignin",
    "territory",
    "registry",
    "program"
  ],
  "author": "Nick Gal√°n - email: gnick.alan@gmail.com",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/node": "20.5.4",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "copyfiles": "2.4.1",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "onchange": "7.1.0",
    "prettier": "3.0.2",
    "prisma": "5.3.1",
    "rimraf": "5.0.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@prisma/client": "5.3.1",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "zod": "3.22.2"
  }
}
